{
  "semi": true,
  "trailingComma": "none",
  "jsxSingleQuote": true,
  "printWidth": 120,
  "singleQuote": true,
  "quoteProps": "consistent",
  "plugins": ["prettier-plugin-astro", "prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": ["*.tsx"],
      "options": {
        "jsxSingleQuote": true
      }
    },
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
